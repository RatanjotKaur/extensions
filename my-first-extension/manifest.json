{
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "web_accessible_resources": [
        {
          "resources": [ "images/logo.png" ],
          "matches": [ "https://*.google.com/*" ]
        }
      ],
    "content_scripts": [
        {
            "matches": ["https://*.google.com/*"],
            "js": ["content-script.js"]
        }
    ]    
  }